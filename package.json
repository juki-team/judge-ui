{
  "name": "judge-ui",
  "private": true,
  "version": "1.17.1",
  "scripts": {
    "dev": "next dev -p 3070",
    "dev:ssl": "next dev -p 3001 & local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint",
    "pull": "yarn add @juki-team/commons @juki-team/base-ui",
    "git-push": "f() { param=${1}; message=${param:-chore}; echo ${message}; git add .; echo $message | git commit -F -; yarn version --patch --no-git-tag-version; version=\"v$(node -p \"require('./package.json').version\")\"; git add .; echo $version | git commit -F -; git push origin main; echo $version | git tag -a $(echo $version) -F -; git push origin --tags; }; f",
    "deploy": "f() { yarn build && yarn git-push \"${1}\"; }; f"
  },
  "dependencies": {
    "@juki-team/base-ui": "^1.0.189",
    "@juki-team/commons": "^0.0.751",
    "@vercel/analytics": "^1.2.2",
    "deep-object-diff": "^1.1.7",
    "diff2html": "^3.4.48",
    "immutability-helper": "^3.1.1",
    "motion": "^11.15.0",
    "next": "^15.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.3",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.1",
    "local-ssl-proxy": "^2.0.5",
    "sass": "^1.71.1",
    "typescript": "^5.4.3"
  }
}
