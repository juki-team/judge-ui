{
  "name": "judge-ui",
  "private": true,
  "version": "1.21.1",
  "scripts": {
    "dev": "next dev -p 3070",
    "dev:ssl": "next dev -p 3001 & local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint",
    "pull": "yarn add @juki-team/commons @juki-team/base-ui",
    "push": "f() { param=${1}; message=${param:-chore}; echo ${message}; git add .; echo $message | git commit -F -; yarn version --patch --no-git-tag-version; version=\"v$(node -p \"require('./package.json').version\")\"; git add .; echo $version | git commit -F -; git push origin main; echo $version | git tag -a $(echo $version) -F -; git push origin --tags; }; f"
  },
  "dependencies": {
    "@juki-team/base-ui": "^1.1.68",
    "@juki-team/commons": "^0.0.874",
    "@reactour/tour": "^3.8.0",
    "@vercel/analytics": "^1.2.2",
    "next": "^15.4.1",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.1",
    "@types/dom-to-image": "^2.6.7",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.1",
    "local-ssl-proxy": "^2.0.5",
    "sass": "^1.89.1",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@types/minimatch": "^5.0.0"
  }
}
